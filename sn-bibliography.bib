@article{zhou_how_2012,
	title = {{HOW} {KNOWLEDGE} {AFFECTS} {RADICAL} {INNOVATION}: {KNOWLEDGE} {BASE}, {MARKET} {KNOWLEDGE} {ACQUISITION}, {AND} {INTERNAL} {KNOWLEDGE} {SHARING}},
