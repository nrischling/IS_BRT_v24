@article{cheng_multi-agent_2022,
	title = {Multi-agent reinforcement learning via knowledge transfer with differentially private noise},
	volume = {37},
	issn = {0884-8173},
	url = {https://doi.org/10.1002/int.22648},
	doi = {10.1002/int.22648},
	abstract = {Abstract In multi-agent reinforcement learning, transfer learning is one of the key techniques used to speed up learning performance through the exchange of knowledge among agents. However, there are three challenges associated with applying this technique to real-world problems. First, most real-world domains are partially rather than fully observable. Second, it is difficult to pre-collect knowledge in unknown domains. Third, negative transfer impedes the learning progress. We observe that differentially private mechanisms can overcome these challenges due to their randomization property. Therefore, we propose a novel differential transfer learning method for multi-agent reinforcement learning problems, characterized by the following three key features. First, our method allows agents to implement real-time knowledge transfers between each other in partially observable domains. Second, our method eliminates the constraints on the relevance of transferred knowledge, which expands the knowledge set to a large extent. Third, our method improves robustness to negative transfers by applying differentially exponential noise and relevance weights to transferred knowledge. The proposed method is the first to use the randomization property of differential privacy to stimulate the learning performance in multi-agent reinforcement learning system. We further implement extensive experiments to demonstrate the effectiveness of our proposed method.},
	pages = {799--828},
	number = {1},
	journaltitle = {International Journal of Intelligent Systems},
	shortjournal = {International Journal of Intelligent Systems},
	author = {Cheng, Zishuo and Ye, Dayong and Zhu, Tianqing and Zhou, Wanlei and Yu, Philip S. and Zhu, Congcong},
	urldate = {2023-01-31},
	date = {2022-01-01},
	note = {Publisher: John Wiley \& Sons, Ltd
tex.ids= {chengMultiAgentReinforcement}2022},
	keywords = {differential privacy, multi-agent learning, transfer learning},
}

