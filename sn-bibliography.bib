	shortjournal = {{ISME} J},
	author = {Anthony, Mark A. and Crowther, Thomas W. and van der Linde, Sietse and Suz, Laura M. and Bidartondo, Martin I. and Cox, Filipa and Schaub, Marcus and Rautio, Pasi and Ferretti, Marco and Vesterdal, Lars and De Vos, Bruno and Dettwiler, Mike and Eickenscheidt, Nadine and Schmitz, Andreas and Meesenburg, Henning and Andreae, Henning and Jacob, Frank and Dietrich, Hans-Peter and Waldner, Peter and Gessler, Arthur and Frey, Beat and Schramm, Oliver and van den Bulk, Pim and Hensen, Arjan and Averill, Colin},
